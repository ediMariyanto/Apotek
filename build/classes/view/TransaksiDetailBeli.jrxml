	<parameter name="Par_Nomor_Nota" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
  Tgl_Trans,
  Kd_Trans,
  Nomor_Nota,
  t.Kd_Barang,
  b.Merk_Barang,
  t.Harga,
  Jumlah,
  t.Harga*Jumlah AS Total,
  d.Nama as Distributor
FROM transaksi_beli t
LEFT JOIN daftar_barang b
ON t.Kd_Barang=CONCAT(b.Kd_Barang,b.Kd_Barang_Detail)
LEFT JOIN Distributor d
ON t.Distributor=d.Kd_Dist
Where Nomor_Nota=$P!{Par_Nomor_Nota}]]>
	</queryString>
